{
  "data_path": "mnist_images",
  "model_name": "ResNet18",
  "epochs": 10,
  "learning_rate": 0.001,
  "batch_size": 512,
  "train_split": 0.7,
  "val_split": 0.2,
  "test_split": 0.1,
  "data_subset": "all",
  "pretrained_model_path": "",
  "advanced_options": {
    "solver": "adam",
    "momentum": 0.9,
    "weight_decay": 0.0,
    "loss": "CrossEntropyLoss",
    "scheduler": "none",
    "lr_drop_factor": 0.1,
    "lr_drop_period": 10,
    "patience": 0,
    "shuffle": "every-epoch",
    "metric": "Accuracy",
    "fscore_beta": 1.0,
    "hflip": false,
    "vflip": false,
    "rotation": false,
    "class_balance_ratio": 1.0,
    "checkpoint_enabled": false,
    "checkpoint_threshold": 0.5,
    "checkpoint_dir": "",
    "tracking_enabled": true,
    "tracking_frequency": 1,
    "tracked_metrics": ["Accuracy"],
    "tracking_fscore_beta": 1.0
  },
  "output_dir": "./output",
  "export_model": true,
  "export_metrics": true
}
